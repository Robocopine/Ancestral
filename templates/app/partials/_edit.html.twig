<!-- Modal -->
<div class="modal fade" id="updateModal{{modalId}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <form method="post" action="{{ path( linkUpdate, paramsUpdate) }}">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close border border-light" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true"><b>X</b></span></button>
                {#}<h4 class="modal-title text-center"><h1 class="h5 font-weight-normal">{% trans %}Connexion{% endtrans %}</h1></h4>{#}
            </div>
            <div class="modal-body">
                <iframe frameborder="0" scrolling="no" border="0" cellspacing="0" width="100%" src="{{ path(linkUpdate, paramsUpdate)}}"></iframe>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-bs-dismiss="modal">{% trans %}Fermer{% endtrans %}</button>
            </div>
        </div>
    </form>
  </div>
</div>