{# templates/app/product/partials/_rating.html.twig -- Rating of a product  #}

<span class="text-warning">
    {% for i in 1..5 %}
        {% if i <= rating %}
            <i class="fas fa-star"></i>
        {% else %}
            <i class="far fa-star"></i>
        {% endif %}
    {% endfor %}
</span>